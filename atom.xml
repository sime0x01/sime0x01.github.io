<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Gabriel Simeoni (aka sime0x01)</title>
 <link href="http://sime0x01.github.io/atom.xml" rel="self"/>
 <link href="http://sime0x01.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://sime0x01.github.io</id>
 <author>
   <name>Gabriel Simeoni (aka sime0x01)</name>
   <email>yestorootkit@protonmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://sime0x01.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://sime0x01.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
